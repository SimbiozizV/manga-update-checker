{
  "name": "Manga update checker",
  "description": "Проверка обновлений манги",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Manga update checker - проверка обновлений манги"
  },
  "permissions": [
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "*://*/*"
  ]
}
